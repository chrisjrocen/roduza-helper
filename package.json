{
    "name": "mosaic-gallery",
    "version": "2.0.1",
    "description": "Plugin with funcitonality for Roduza Collections",
    "main": "index.js",
    "scripts": {
        "slot-start": "wp-scripts start --webpack-src-dir=admin/slotfill --output-path=assets/js/slotfill",
        "slot-build": "wp-scripts build --webpack-src-dir=admin/slotfill --output-path=assets/js/slotfill",
        "admin-start": "wp-scripts start --webpack-src-dir=admin/pre-publish-checks --output-path=assets/js/pre-publish-checks",
